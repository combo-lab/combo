      "import": "./priv/static/html.esm.js",
      "require": "./priv/static/html.cjs.js"
    }
  },
  "files": [
    "LICENSE.md",
    "package.json",
    "priv/static/*"
  ],
  "devDependencies": {
    "@babel/cli": "7.28.0",
    "@babel/core": "7.28.0",
    "@babel/preset-env": "7.28.0",
    "@eslint/js": "^9.28.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "documentation": "^14.0.3",
    "esbuild": "^0.25.8",
    "eslint": "9.30.1",
    "eslint-plugin-jest": "29.0.1",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jsdom": "^26.1.0",
    "mock-socket": "^9.3.1"
  },
  "scripts": {
    "test": "jest",
    "test.coverage": "jest --coverage",
    "test.watch": "jest --watch",
    "docs": "documentation build assets/src/socket/index.js -f html -o doc/js"
